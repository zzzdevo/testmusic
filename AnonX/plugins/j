def save(data):
    with open("rd.json", "w", encoding='utf-8') as f:
        json.dump(data, f, indent=6, ensure_ascii=False)
        f.close()


def ck(c):
    try:
        da[c]
    except KeyError:
        da[c] = {}
        save(da)


