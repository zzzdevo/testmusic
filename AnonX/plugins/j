@app.on_message(filters.regex("^Ø²ÛŒØ§Ø¯Ú©Ø±Ø§ÙˆÛ•Ú©Ø§Ù†$"))
async def t(client, m):
    r = ""
    i = 0
    cid = str(m.chat.id)
    ck(cid)
    if da[cid] != {}:
        for a, b in da[cid].items():
            tp = b.split("&", 1)
            if tp[0] == "text":
                t = "**ÙˆØ´Û•**"
            elif tp[0] == "photo":
                t = "**ÙˆÛÙ†Û•**"
            elif tp[0] == "video":
                t = "**Ú¤ÛŒØ¯ÛŒÛ†**"
            elif tp[0] == "animation":
                t = "**Ú¯ÛŒÙ**"
            elif tp[0] == "voice":
                t = "**Ú¤Û†ÛŒØ³**"
            elif tp[0] == "audio":
                t = "**Ú¯Û†Ø±Ø§Ù†ÛŒ**"
            elif tp[0] == "document":
                t = "**ÙØ§ÛŒÙ„**"
            i += 1
            r += f'{i} => {a} ~ {t}\n'
        await m.reply(r)
    else:
        await m.reply("**Ù‡ÛŒÚ† Ú†Ø§ØªÛÚ©ÛŒ Ø²ÛŒØ§Ø¯Ú©Ø±Ø§Ùˆ Ù†ÛŒÛŒÛ•â™¥ï¸**â€¢")


